{
    "name": "Tec extensions finder",
    "manifest_version": 2,
    "description": "Chrome extension to find TEC worker's phone extensions",
    "version": "1.0",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Extension Finder"
    }
}